       		<script type="text/javascript">
       			       			$(".tabs a[data-naimenovanie='anker_bolt']").addClass('current');
       			
				       			
       			function show_tabs_group(group_id){
       				$('li[data-group-id="'+group_id+'"]').toggle();
       			}

       			       				show_tabs_group( 4 );
       			

				function sizes_loading_message_show(){
					if( !sizes_is_loading || $('.sizes_loading_message').length ) return;
					$('body').append('<div class="sizes_loading_message" style="left:'+($(window).width()/2-50)+'px;">Загрузка...</div>');
					yandex_counter_hit('SIZES_LONG_LOAD'); 
				}

				var sizes_is_loading = 0;
				$("ul.tabs a").click(function(event){

										event.preventDefault ? event.preventDefault() : event.returnValue = false;

					if( sizes_is_loading ) return;
					else sizes_is_loading = 1;

					$('.sizes_loading_message_error').remove();

					$("ul.tabs a").removeClass('current');
					$(this).addClass('current');
					$("#show_services").css('font-weight','normal').css('color','#0063dc');

					var naim = $(this).attr('data-naimenovanie');

					var pane = $(".pane[data-naimenovanie='"+naim+"']");
					if( pane.length === 1 ){
						sizes_is_loading = 0;
						$('.sizes_loading_message').remove();
						$(".pane").removeClass('current');
						pane.addClass('current');
						$("div.panes").scrollTop(0);
						return;	
					}

					setTimeout( sizes_loading_message_show , 1200);
					var slms_2000 = setTimeout( function(){yandex_counter_hit('SIZES_LONG_LOAD_2000'); } , 2000);
					var slms_3000 = setTimeout( function(){yandex_counter_hit('SIZES_LONG_LOAD_3000'); } , 3000);

					$.post("https://23met.ru/price", { ajax: 'sizes', n : naim }).done(
					  function(sizes){
					  	sizes_is_loading = 0;
					  	$('.sizes_loading_message').remove();
					  	clearTimeout(slms_2000);
					  	clearTimeout(slms_3000);

					  	$(".pane").removeClass('current');

					  	if( !$(".pane[data-naimenovanie='"+naim+"']").length ){
					  		$(".panes").append(sizes);
					  	}

					  	$(".pane[data-naimenovanie='"+naim+"']").addClass('current');
					  	$("div.panes").scrollTop(0);

					  	
					}).fail(function(xhr, status, error) {

        				sizes_is_loading = 0;
        				$('.sizes_loading_message').remove();
        				clearTimeout(slms_2000);
        				clearTimeout(slms_3000);

        				$('body').append('<div class="sizes_loading_message_error" style="left:'+($('#left-container').offset().left + $('#left-container').width())+'px;top:'+( $("ul.tabs a.current").offset().top)+'px;width:auto;">Произошла ошибка – не удалось загрузить данные.<br />Пожалуста, попробуйте еще раз или <a onclick="javascript:window.location.reload(true)" style="cursor:pointer; text-decoration:underline;color:#fc3;">перезагрузите страницу</a></div>');

        				$('.sizes_loading_message_error').on('click', function(){ $(this).remove(); })

        				$("ul.tabs a").removeClass('current');
        				$(".pane").removeClass('current');
						
        				switch (xhr.status) {
					        case 500:
					            yandex_counter_hit('SIZES_LOAD_FAIL_500');
					            break;
					        case 400:
					            yandex_counter_hit('SIZES_LOAD_FAIL_400');
					            break;
					        case 0:
					            yandex_counter_hit('SIZES_LOAD_FAIL_0');
					            break;
					        default:
					            yandex_counter_hit('SIZES_LOAD_FAIL');
					            break;
					    }

        				
   					});

				});

								$(document).ready(function(){ 
				   	$(".tab-group-shower").click(function(event){
							show_tabs_group( $(this).attr('data-group-id') );
							resize_center_container(null);
					});
				});
       		</script>
       		<div style="clear:both; margin-bottom: 12px;"></div>
    	
    			<ul class="tabs-sorting-showing-selector-container">
		  <li class="left selected " onclick="set_tabs_sorting_type('az',this)">А-Я</li><li class="right  " onclick="set_tabs_sorting_type('gr',this)">Группы</li>
		</ul>
		
		<ul id="sizes-showing-mode-container" class="tabs-sorting-showing-selector-container">
		  <li class="left selected" onclick="set_tabs_and_sizes_showing_type('all',this)" >Все города</li><li class="right " onclick="set_tabs_and_sizes_showing_type('current_cities',this)">Текущий</li><li class="help">?</li>
		</ul>
				
