<script type="text/javascript">
$(document).ready(function(){ 	
	var rotate_banners_interval;
	set_rotate_banners_interval();
	
	function rotate_banners(){
		var hnc_class = "j0b95a";
		var snc_class = "jee850";
		var b_class = "jc88bb";

		var hnc = $( '.' + hnc_class);
		var snc = $( '.' + snc_class);

		hnc.removeClass( hnc_class );
		snc.removeClass( snc_class ).addClass( hnc_class );

		$(snc).each(function(i) {
			if( $(this).next().length ) $(this).next().addClass( snc_class );
			else $(this).parent().find('.' + b_class).first().addClass( snc_class );	
		});
	}


	$(document).on('click', '#j140ed, #ja0d10', function (){
		clearInterval(rotate_banners_interval);
		$('#jd25b3').remove();

		var b, txt;
		if( $(this).attr('id')      === 'j140ed'    ) b = $('#jca09e')
		else if( $(this).attr('id') === 'ja0d10' ) b = $('#j55120')
		else return;

		if(      b.find('.jff8c1').length ) txt = b.find('.jff8c1').find('img').attr('data-text');
		else if( b.find('.j0b95a').length )         txt = b.find('.j0b95a').find('img').attr('data-text');
		else return;

	  	$('body').append('<div id="jd25b3">' + txt + '</div>');
	  	$('#jd25b3').css('top', $(this).offset().top + $(this).outerHeight() - parseInt($(this).css('padding-bottom')) + 7).css('left', $(this).offset().left + $(this).outerWidth() - $('#jd25b3').outerWidth() - 7);
	});	

	$(document).on('mouseover', '#j140ed, #ja0d10, #jca09e, #j55120', function (){
		clearInterval(rotate_banners_interval);
	});

	$(document).on('mouseleave', '#j140ed, #ja0d10, #jca09e, #j55120', function (){
		$('#jd25b3').remove();
		set_rotate_banners_interval();
	});

	function set_rotate_banners_interval(){
		rotate_banners_interval = setInterval(function() { rotate_banners() }, 8100);
	}

});

$(window).load(function() {
	$('#jca09e img[data-src], #j55120 img[data-src]').each(function(i) {
	  $(this).attr('src', $(this).attr('data-src') );
	});
});
</script>

