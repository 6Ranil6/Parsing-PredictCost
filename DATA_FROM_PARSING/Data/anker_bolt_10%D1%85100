$(document).ready(function(){ 
	$(".citychooser_opener").on('click', function(e){	
		if( $(this).prop("tagName") === 'A' ) e.preventDefault ? e.preventDefault() : e.returnValue = false;
		show_citychooser_popup(1);
	});	

		

	$(".header_cities_list-showing_type-selector-help").on('click', function() {	
	  	show_help_popup(1, '<b>!!</b> Данная настройка влияет только на способ отображения списка городов.<br /><br /><b>Все города</b> &mdash; отображать все доступные города, выбранные выделены <b>жирным</b>.<br /><br /><b>Текущий</b> &mdash; отображать только выбранный город(а).<br /><br />Для отображения продукции из нескольких городов необходимо отметить их во всплывающем окне, нажав <i>Выбрать несколько</i>.', this, 300);
	  	$(this).on('mouseleave', function(){ show_help_popup(0); });
	});
});



function set_cities_settings_show_all(show_all_cities, el){
	if( $(el).hasClass('header_cities_list-showing_type-selector-current') ) return;
	$.post("https://23met.ru/user", { ajax:'set_show_all_cities_settings', show_all_cities:show_all_cities },
	function(data){
		window.location.reload();
	});
}



var citychooser_show_type = 'az';
function show_citychooser_popup(show){

	if(!show){
		$('#citychooser_popup').hide();
		$('#citychooser_shadow').hide();
		return;
	}

	if( $('#citychooser_popup').length ){
		$('#citychooser_popup').show();
		$('#citychooser_shadow').show();
		citychooser_popup_set_coordinates();
		citychooser_resize_settings_inner();
		return;		
	}

	
	var popup = '<div id="citychooser_popup" data-window-width="'+$(window).width()+'">'
	popup += '<div class="popup-closer-container" onclick="show_citychooser_popup(0)"><div class="popup-closer" onclick="show_citychooser_popup(0)"><svg width="24" height="24" viewBox="0 0 24 24" fill="#f2f2f2"><path d="M15.13 12L21 17.87L18.17 20.73L12.31 14.83L6.36 20.73L3.58 17.87L9.53 12L3.54 6.14L6.36 3.28L12.31 9.18L18.17 3.32L21 6L15.13 12Z"></path></svg></div></div>';
	popup += '<table class="citychooser-tab"><tr>';
	popup += '<td valign="top" class="citychooser-td-togglable citychooser-td-az citychooser-td-left">';
	var cities = [];
	var region_cities = {};
	$.each( multicity_city_rus_TO_params, function(city, p) {
		cities.push(city);
	 	region_cities[p['region']] = '';
	});

	var cities_last_choosed = {};
	multicity_cities_last_choosed = multicity_cities_last_choosed.split(',');

	$.each(multicity_cities_last_choosed, function(i, city){
		cities_last_choosed[ city ] = city; 
	});


	var url_http_and_slashes = 'https://';
	var url_last_part = '23met.ru/price/anker_bolt/10%D1%85100' + window.location.hash;


	var prev_first_letter = null;
	cities.sort();
	$.each(cities, function(index, city){
		var first_letter = city.charAt(0);

		if( first_letter != prev_first_letter ){
			if( first_letter == 'К' || first_letter == 'О' || first_letter == 'Т' ){
				popup += '</td><td valign="top" class="citychooser-td-togglable citychooser-td-az">';
			}
			popup += '<div class="citychooser-first-letter">' + first_letter + '</div>';
		}

		prev_first_letter = first_letter;

		var last_choosed = 0;
		if( cities_last_choosed[ city ] ){
			last_choosed = 1;
		}

		popup += '<div class="citychooser-city-container" data-city="'+ city +'" data-x="'+ multicity_city_rus_TO_params[city]['x'] +'" data-y="'+ multicity_city_rus_TO_params[city]['y'] +'">';
		popup += '<input type="checkbox" class="citychooser" value="'+city+'" id="citychooser-'+ index +'" />';

		popup += '<a href="' + url_http_and_slashes + multicity_city_rus_TO_params[city]['eng'] + ( multicity_city_rus_TO_params[city]['eng'] === '' ? '' : '.' ) + url_last_part + '" class="citychooser-city-link" data-last-choosed="' + last_choosed + '" data-region="' + multicity_city_rus_TO_params[city]['region']+ '">'+city+'</a>';
		if( new_cities[city] ){
			popup += '<small class="citychooser-city-new">новый</small>';
		}
		popup += '</div>';

		region_cities[ multicity_city_rus_TO_params[city]['region'] ] += '<span class="citychooser-replacer" data-city="'+city+'"></span>';
	});
	popup += '</td>';


	var regions_sort = ['Северо-Запад', 'Центр', 'BR', 'Черноземье', 'Юг', 'Северный Кавказ', 'BR', 'Поволжье', 'BR', 'Урал', 'Сибирь', 'Дальний Восток'];
	popup += '<td valign="top" class="citychooser-td-togglable citychooser-td-region citychooser-td-left" style="display:none;">';
	$.each(regions_sort, function(index, region){

		if( region =='BR' ) popup += '</td><td valign="top" class="citychooser-td-togglable citychooser-td-region" style="display:none;">';
		else popup += '<div><div class="citychooser-region-title">' + region + '</div>' +region_cities[region] + '</div>';
	});
	popup += '</td>';


	popup += '<td valign="top" class="citychooser-td-togglable citychooser-td-map citychooser-td-left" style="display:none; padding:0 !important;">';
	popup += '<div id="citychooser-map" style="width:1100px; height:634px; background:url(https://23met.ru/img/cities_map_2023.png) no-repeat #c3e1c8; background-size: 1100px 634px;">';
	$.each(cities, function(index, city){
		popup += '<span class="citychooser-replacer" data-city="'+city+'"></span>';
	});
	popup += '</div>';
	popup += '</td>';


	popup += '<td class="citychooser-settings-td"><div class="citychooser-settings-td-inner">';

	popup += '<span onclick="swap_az_to_regions(this, \'az\')" class="citychooser_sort-type citychooser_sort-type-current">А-Я</span>';
	popup += '<span onclick="swap_az_to_regions(this, \'region\')" class="citychooser_sort-type" >Регионы</span>';
	popup += '<span onclick="swap_az_to_regions(this, \'map\')" class="citychooser_sort-type" >Карта</span>';

	popup += '<div class="citychooser_popup_horizontal_separator"></div>';

	popup += '<input id="regionchooser-all" class="regionchooser" type="checkbox" value = "Все" />';
	popup += '<label for="regionchooser-all" class="regionchooser-label">Все</label>';
	popup += '<div style="height:5px;"></div>';

	var regions_az = Object.keys(region_cities).sort();
	$.each(regions_az, function(ri, rv){
		if( ri !== 0 ) popup += '<div style="height:5px;"></div>';
		popup += '<input id="regionchooser-'+ri+'" class="regionchooser" type="checkbox" value = "'+rv+'" />';
		popup += '<label for="regionchooser-'+ri+'" class="regionchooser-label">'+rv+'</label>';
	});

	popup += '<div class="citychooser_popup_horizontal_separator"></div>';


	var radius_from_city = null;
	if( multicity_cities_current.length === 1 ){
		radius_from_city = multicity_cities_current[0];
	}

	popup += '<div id="get_cities_in_radius_from_city-city-container">';
	popup += '<select id="get_cities_in_radius_from_city-city">';
	popup += (radius_from_city)? '' : '<option selected disabled hidden>Город</option>';
	$.each(cities, function(i, city){
		var option_is_selected = ( radius_from_city && city === radius_from_city )? 'selected' : '';
		popup += '<option ' + option_is_selected + '>' + city + '</option>';
	});
	popup += '</select><div id="get_cities_in_radius_from_city-city-label" data-text="Выбрать город">'+((radius_from_city)? radius_from_city : '')+'</div></div><div class="get_cities_in_radius_from_city-text">+</div><div id="get_cities_in_radius_from_city-radius-container"><select id="get_cities_in_radius_from_city-radius" value="" '+((radius_from_city)? '' : 'style="display:none"')+'></select><div id="get_cities_in_radius_from_city-radius-label" data-text="города в радиусе"></div></div>';

		
	popup += '<div class="citychooser_popup_horizontal_separator"></div>';
	popup += '<span class="citychooser-pseudolink" onclick="citychooser_clear()">Сбросить все</span>';

	popup += '<div class="citychooser_popup_horizontal_separator"></div>';
	popup += '<input onclick="citychooser_save_cities_list_for_user()" class="button citychooser-save-btn" type="button" value="Сохранить" style="width:100%;" />';
	popup += '</div></td>';
	popup += '</tr></table>';

	popup += '<div id="citychooser-message-error"></div>';

	popup += '</div>';/*popup*/


	$('body').append( popup );


	citychooser_popup_set_coordinates();


	$('body').append('<div id="citychooser_shadow" onclick="show_citychooser_popup(0)"></div>');
    $('#citychooser_shadow').css('width', '100%').css('height', '100%');

	$.each(multicity_cities_current, function(i, city){
		add_city_choosed( city, true );
	});

	update_regions_checkbox_state();

	$(".citychooser").on('click', function(e){			
	  	citychooser_show_error();

	  	var city = $(this).val();

	  	invert_city_choosed( city, false );
	  	update_regions_checkbox_state();

	  	if(e.screenX && e.screenX != 0 && e.screenY && e.screenY != 0){
	  		drop_cities_in_radius_value();
	  	}
	});

	$(".regionchooser").bind('click', function(e){		
		citychooser_show_error();	
	  	
	  	var el_region = $(this).val();
		var set_checked = $(this).is(':checked');

		$.each(multicity_city_rus_TO_params, function(city, p){
			if( el_region !== 'Все' && p['region'] !== el_region ) return;

			if( set_checked ) add_city_choosed( city, true );
			else delete_city_choosed( city, true );
		});

		update_regions_checkbox_state();
		drop_cities_in_radius_value();
	});

	$("#get_cities_in_radius_from_city-city").on('change', function(e){	
		$('#get_cities_in_radius_from_city-city-label').text( $(this).val() );
		load_city_to_distance();
	  	$(this).blur();
	});	

	$("#get_cities_in_radius_from_city-radius-label").on('click', function(e){	
		if( !$('#get_cities_in_radius_from_city-city').val() ) blink_border('#get_cities_in_radius_from_city-city-container');
	});	

	$('#citychooser-latest').hover(
		function() {
			$('a[data-last-choosed="1"]').addClass('highlighted');
		}, function() {
			$('a[data-last-choosed="1"]').removeClass('highlighted');
		}
	);

	$('.regionchooser').hover(
		function() {
			if( citychooser_show_type === 'region') return;
			$('a[data-region="' + $(this).val() + '"]').addClass('highlighted');
		}, function() {
			$('a[data-region="' + $(this).val() + '"]').removeClass('highlighted');
		}
	);

	$('.regionchooser-label').hover(
		function() {
			if( citychooser_show_type === 'region') return;
			$('a[data-region="' + $('#'+ $(this).attr('for')).val() + '"]').addClass('highlighted');
		}, function() {
			$('a[data-region="' + $('#'+ $(this).attr('for')).val() + '"]').removeClass('highlighted');
		}
	);


	if( $(window).width() < 1270 ){
		$(".citychooser_sort-type:contains('Карта')").hide();
	}
	else{
		$(".citychooser_sort-type:contains('Карта')").show();
	}

	if( citychooser_show_type === 'region' ){
		$(".citychooser_sort-type:contains('Регионы')").click();
	}	
	else if( citychooser_show_type === 'map' ){
		if( $(window).width() < 1270 ){
			$(".citychooser_sort-type:contains('А-Я')").click();
		}
		else{
			$(".citychooser_sort-type:contains('Карта')").click();
		}
	}	

	citychooser_resize_settings_inner();
	load_city_to_distance();
	
	yandex_counter_hit('Citychooser');
}

function citychooser_popup_set_coordinates(){
	var ww = $(window).width();
	var l = ww/2 - $('#citychooser_popup').innerWidth()/2;
	if( l < 0 ) l = 0;
	$('#citychooser_popup').css({'left': l, 'top': $(window).scrollTop() + 20}).attr('data-window-width', ww);
}

function citychooser_resize_settings_inner(){
	$('.citychooser-settings-td-inner').css('height', 'auto');
	$('.citychooser-settings-td-inner').height( $('.citychooser-settings-td').height() );
}

function invert_city_choosed( city, set_checkbox_state ){
  	if( multicity_cities_choosed[ city ] ) delete_city_choosed( city, set_checkbox_state );
  	else add_city_choosed( city, set_checkbox_state );
}
function add_city_choosed( city, set_checkbox_state ){
	multicity_cities_choosed[ city ] = city;
	if( set_checkbox_state ) $('.citychooser[value="'+city+'"]').prop('checked', true);
}
function delete_city_choosed( city, set_checkbox_state ){
	delete( multicity_cities_choosed[ city ]);
	if( set_checkbox_state ) $('.citychooser[value="'+city+'"]').prop('checked', false);
}

function update_regions_checkbox_state(){
	var regions_with_unchecked_city = {};
	var regions_with_checked_city = {};
	var checked_count = 0;
	var unchecked_count = 0;

	$.each(multicity_city_rus_TO_params, function(city, p){
		if( !multicity_cities_choosed[ city ] ){
			regions_with_unchecked_city[ p['region'] ] = 1;
			unchecked_count++;
		}
		else{
			regions_with_checked_city[ p['region'] ] = 1;
			checked_count++;
		}
	});

	$('.regionchooser').each(function(index){
		var region = $(this).val();

		if( region == 'Все' ){
			if( unchecked_count === 0 ){
				$(this).prop('checked', true);
				$(this).prop('indeterminate', false);
			}
			else if( checked_count === 0 ){
				$(this).prop('checked', false);
				$(this).prop('indeterminate', false);
			}
			else{
				$(this).prop('checked', false);
				$(this).prop('indeterminate', true);
			}

		}else{
			if( !regions_with_unchecked_city[region] ){
				$(this).prop('checked', true);
				$(this).prop('indeterminate', false);
			}
			else if( regions_with_checked_city[region] ){
				$(this).prop('checked', false);
				$(this).prop('indeterminate', true);
			}
			else{
				$(this).prop('checked', false);
				$(this).prop('indeterminate', false);
			}
		}
	});	
}


function citychooser_save_cities_list_for_user(){
  $.post("https://23met.ru/user", { ajax: 'save_multicity_cities_and_get_new_url', cities: multicity_cities_choosed },
    function(data){

		data=jQuery.parseJSON(data);

		if(data['error']){
            citychooser_show_error(data['error'])
		} 
		if(data['ok']){
			if( window.location.href === data['redirect_url'] + window.location.hash ){
				window.location.reload();
			}else{
				window.location.href = data['redirect_url'] + window.location.hash; 
			}
		}                 
  });
}

function citychooser_get_last_cities_list(){
  $.post("https://23met.ru/user", { ajax: 'get_multicity_cities_last_list' },
    function(data){

		data=jQuery.parseJSON(data);

		if(data['error']){
            citychooser_show_error(data['error'])
		} 
		if(data['ok']){
			citychooser_clear();

			$.each(data['cities'], function( index, city ) {
			 	$('.citychooser[value="'+ city +'"]').click();
			});

			drop_cities_in_radius_value();
		}                 
  });
}

function get_cities_in_radius_from_city(){
	var city = $('#get_cities_in_radius_from_city-city').val();
	if( !city ){
		blink_border('#get_cities_in_radius_from_city-city-container');
		return;
	}

	var el_r = $('#get_cities_in_radius_from_city-radius');
	var el_rl = $('#get_cities_in_radius_from_city-radius-label');
	var radius = el_r.val();

	radius = radius.replace(/(\d),(\d)/g, '$1.$2');
	if( !radius || /[^\d\.]/.test(radius) ){
		blink_border('#get_cities_in_radius_from_city-radius-container');
		return;
	}

	$.post("https://23met.ru/price", { ajax: 'get_cities_in_radius_from_city',
									    city: city,
									    radius: radius
  									 },
	function(data){
		data=jQuery.parseJSON(data);

		if(data['error'] == 'radius'){
			el_r.val('').find('option:selected').prop('selected', false)
			el_rl.text('');
	        blink_border('#get_cities_in_radius_from_city-radius-container');
			return;
		}
		else if(data['error']){
			alert(data['error']);
			return;
		}
		else if(data['cities']){
			citychooser_clear('no_drop_radius_value');

			$.each(data['cities'], function( index, city ) {
			 	$('.citychooser[value="'+ city +'"]').click().closest('.citychooser-city-container').find('a').addClass('highlighted');
			});
			window.setTimeout(function(){ $('.citychooser-city-container a').removeClass('highlighted'); }, 500);
		}                 
	}).fail(function(xhr, status, error) {
		el_r.val('').find('option:selected').prop('selected', false)
		el_rl.text('');
		blink_border('#get_cities_in_radius_from_city-radius-container');
		yandex_counter_hit('SEARCH_OTHER_CITIES_NOTIF'); 
	});
}

function citychooser_show_error(error_message){
	if( !error_message ){
		$('#citychooser-message-error').hide();
		return;
	}
	$('#citychooser-message-error').text(error_message).show().fadeTo(400, 0.3).fadeTo(400, 1.0).fadeTo(400, 0.3).fadeTo(400, 1.0);
}

function citychooser_clear(no_drop_radius_value){
	$('.citychooser:checked').click();
	$('.citychooser-city-container a').removeClass('highlighted');
	multicity_cities_choosed = {};
	if(no_drop_radius_value) return;
	drop_cities_in_radius_value();
}

function swap_az_to_regions(e, type){
	if( $(e).hasClass('citychooser_sort-type-current') ) return;

	$('.citychooser-city-container').each(function(){
		var replacer = $('.citychooser-td-'+type+' .citychooser-replacer[data-city="' + $(this).attr('data-city') + '"]');

		$(this).clone(true).insertBefore( replacer );
		$(this).replaceWith( replacer );
	});

	$('.citychooser-td-togglable').hide();
	$('.citychooser-td-'+type).show();
	$('.citychooser_sort-type').removeClass('citychooser_sort-type-current');
	$(e).addClass('citychooser_sort-type-current');


	if( $(e).is(':contains("А-Я")') ){
		citychooser_show_type = 'az';

		$('.citychooser-city-container').each(function(){
			$(this).css({ 'left':'auto', 'top':'auto' });
		});
	}
	else if( $(e).is(':contains("Регионы")') ){
		citychooser_show_type = 'region';

		$('.citychooser-city-container').each(function(){
			$(this).css({ 'left':'auto', 'top':'auto' });
		});
	}
	else if( $(e).is(':contains("Карта")')   ){
		citychooser_show_type = 'map';

		$('.citychooser-city-container').each(function(){
			$(this).css({ 'left':$(this).data('x'), 'top':$(this).data('y') });
		});
	}
	
	citychooser_popup_set_coordinates();
	citychooser_resize_settings_inner();

	$.post("https://23met.ru/user", { ajax:'set_citychooser_show_type', citychooser_show_type:citychooser_show_type },
	  function(data){});
}


function load_city_to_distance(){
	var el_c = $('#get_cities_in_radius_from_city-city');
	var el_cl = $('#get_cities_in_radius_from_city-city-label');
	var city = el_c.val();
	if( city === null || city === undefined || !city ) return;

	var el_r = $('#get_cities_in_radius_from_city-radius');
	var el_rl = $('#get_cities_in_radius_from_city-radius-label');

	$.post("https://23met.ru/price", {  ajax:'get_city_array_ordered_by_distance_to_given_city', city: city },
		  function(data){

		  	data = jQuery.parseJSON(data);

	        if( data['city_to_distance'] !== undefined ){

				var ctd = Array(data['city_to_distance']);
				ctd = ctd.sort(function(a,b){ return (a > b) ? 1 : ((b > a) ? -1 : 0);});

				el_r.html('').show();
				el_r.append('<option value="empty" selected disabled hidden>Радиус</option>');
				el_r.append('<option value="0">0 км</option>');
				$.each(ctd[0], function(c, d) {
					el_r.append('<option value="'+d+'">'+d+' км - '+c+'</option>');
				});

				el_rl.text('');

				$(el_r).off('.gcirfc');
				$(el_r).on('change.gcirfc', function(){
					el_rl.text( $(this).val()+' км');
					get_cities_in_radius_from_city();
				});
		  	}
		  	else{
				if(data['error']){
					alert(data['error']);
				}

		  		$(el_c).val('').find('option:selected').prop('selected', false);
		  		el_cl.html('');
		  		el_r.html('').hide();
		  		el_rl.text('');
	        	blink_border('#get_cities_in_radius_from_city-city-container');
	        	yandex_counter_hit('SEARCH_OTHER_CITIES_NOTIF'); 
	        }
	}).fail(function(xhr, status, error) {
		$(el_c).val('').find('option:selected').prop('selected', false);
		el_cl.html('');
		el_r.html('').hide();
		el_rl.text('');
		blink_border('#get_cities_in_radius_from_city-city-container');
		yandex_counter_hit('SEARCH_OTHER_CITIES_NOTIF'); 
	});
}

function drop_cities_in_radius_value(){
	var el_r = $('#get_cities_in_radius_from_city-radius');
	if( el_r.val() !== 'empty' && el_r.val() !== null ){
		el_r.val('empty');
		$('#get_cities_in_radius_from_city-radius-label').text('');
	}
}


$(window).resize(function(){ 
	if($('#citychooser_popup').attr('data-window-width') == $(window).width()) return;
	show_citychooser_popup(0); 
});
