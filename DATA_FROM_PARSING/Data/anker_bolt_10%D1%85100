		    	<script type="text/javascript">
	    		$(document).ready(function(){ 
					
					$('.td_cost, .td_cost2').on('click', function(e){
						show_ediz_value_popup(this)
						$(document).on('click touchend', function(e){  if( !$(e.target).closest('#ediz_value_popup, .td_cost, .td_cost2').length ) show_ediz_value_popup(0); });
						$(window).resize(function(){ show_ediz_value_popup(0); });
					});

					function show_ediz_value_popup(td){

						$('#ediz_value_popup').remove();
						if( !td ) return;

						var el = $(td).find("[class*='table_title_ediz_counter_cost']");
						if( !el.length ) return;
						
						$('body').append('<div id="ediz_value_popup"><div id="ediz_value_popup-input-container"><input id="ediz_value_popup-input" type="text" data-ediz="'+el.attr('data-ediz')+'" value="" inputmode="decimal" /><label id="ediz_value_popup-label" for="ediz_value_popup-input">'+el.attr('data-ediz')+'</label></div> <button id="ediz_value_popup-button" class="button">Сохранить</button></div>');

						var p = $('#ediz_value_popup');						
						p.css('top', el.offset().top + el.outerHeight() + 1).css('left', el.offset().left + el.outerWidth() / 2 - p.width() / 2);

						var d = p.offset().left + p.outerWidth() - $(window).width() - $(document).scrollLeft();
						if( d > 0 ) p.css('left', parseInt(p.css('left'), 10) - d - 4);
						if( p.offset().left < 0 ) p.css('left', 1);

						p.find('input').focus().val( el.text() );		

						$('#ediz_value_popup-button').on('click', function(e){
							if( !ediz_value_is_valid() ) return;
							update_table_head_ediz_count(1);
							show_ediz_value_popup(0);
						});		

						$('#ediz_value_popup-input').on('keydown', function(e){ 
							if(e.which != 13) return; 
							if( !ediz_value_is_valid() ) return;
							update_table_head_ediz_count(1);
							show_ediz_value_popup(0);
						});	

						function ediz_value_is_valid(){
							var v = $('#ediz_value_popup-input').val();
							v = v.trim().replace(/,/g, '.').replace(/\.$/g, '');
							if( !v.match(/^[\d]+(\.[\d]+)?$/g) ){
						  		blink_border('#ediz_value_popup-input-container');
						  		return 0;
						  	}
						  	return 1;							
						}
					}

				});
